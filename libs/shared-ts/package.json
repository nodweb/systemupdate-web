{
  "name": "systemupdate-shared-ts",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "openapi-typescript": "^7.5.0",
    "eslint": "^9.9.0",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "generate": "npm run generate:auth && npm run generate:device",
    "generate:auth": "openapi-typescript ../../libs/proto-schemas/openapi/auth.yaml -o ./types/auth.d.ts",
    "generate:device": "openapi-typescript ../../libs/proto-schemas/openapi/device.yaml -o ./types/device.d.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  }
}
